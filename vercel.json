{
  "functions": {
    "api/mercadopago-create-payment.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/mercadopago-webhook.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/resend/emails",
      "destination": "/api/resend-emails"
    },
    {
      "source": "/api/mercadopago/create-payment",
      "destination": "/api/mercadopago-create-payment"
    },
    {
      "source": "/api/mercadopago/webhook",
      "destination": "/api/mercadopago-webhook"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
